/*按钮*/
 .form_last{
  position: relative;
  width: 84%;
  height: 90rpx;
  line-height: 90rpx;
  margin: 0px auto;
  margin-top: 50rpx;
  text-align: center;
  font-size: 28rpx;
  background-color: #EACF02;
  color: #fff;
}

.form_last:active{
  background-color: #EDE03A;
}

button::after {
  content: " ";
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: 0px solid #fff;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 10px;
}